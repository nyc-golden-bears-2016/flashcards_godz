<h1>Welcome <%= @user.username %>!!!!</h1>

<h2> Choose your fate, if you dare</h2>
<main role="main">
  <ul class="entries-list no-bullets no-padding">
<% Deck.all.each do |deck| %>

      <li class="entry-item pad-b-4">
        <a href="/decks/<%= deck.id %>" class="entry-title font-2 pad-b-1-4 c-white"><%= deck.name %></a>
      </li>
<% end %>

  </ul>
</main>
